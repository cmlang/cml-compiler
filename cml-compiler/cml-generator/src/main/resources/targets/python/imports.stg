
concept_file_import_list(concept) ::= <<
from typing import *
from abc import *

<concept.dependencies:concept_file_import(); separator="\n">
>>

concept_file_import(name) ::= <<
from .<python_file_name(name)> import <type_name(name)>
>>

python_file_name(name) ::= <<
<name; format="underscore-case">
>>

