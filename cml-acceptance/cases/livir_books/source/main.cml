module livir_books
{
    import livir_products;
}

concept Book: Product
{
    isbn: String;
    title: String;
}

concept BookStore;

target livir_poj
{
    type = "poj";
    groupId = "livir";
    artifactId = "livir-books";
    version = "1.0.1-SNAPSHOT";
    packageName = "livir.books";
    packagePath = "livir/books";
}

target livir_pop
{
    type = "pop";
    version = "1.0.1";
    moduleName = "livir_books";
}

