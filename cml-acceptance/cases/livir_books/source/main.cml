module livir_books
{
    import livir_products;
}

concept Book: Product
{
    isbn: String;
    title: String;
}

concept BookStore
{
    books: Book*;
    customers: Customer*;
}

task livir_poj
{
    type = "poj";
    groupId = "livir";
    artifactId = "livir-books";
    artifactVersion = "1.0.1-SNAPSHOT";
    packageName = "livir.books";
    packagePath = "livir/books";
}

task livir_pop
{
    type = "pop";
    moduleName = "livir_books";
    moduleVersion = "1.0.1";
}

