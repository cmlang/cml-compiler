// design/cmlc

class_self_field(concept) ::= <<
<if(concept.associationProperties)><\\>
<field_decl(actual_self_decl(concept))><\\>
<endif>
>>

extend_invocation(redefined) ::= <<
<field_access(redefined.concept)> = <\\>
<type_name(redefined.concept)>.<interface_extend_name(type_name(redefined.concept))><\\>
(<if(redefined.concept.associationProperties)><actual_self_field(concept)>, <endif><extend_invocation_extra_args(redefined.concept)>)<statement_terminator()>
>>


