
concept Vehicle
{
    plate: String;
    driver: Employee?;
    owner: Organization;
}

concept Organization
{
    name: String;
    employees: Employee*;
}

concept Employee
{
    name: String;
}

task vehicles_poj by poj
{
    groupId = "vehicles";
    artifactId = "vehicles-poj";
    artifactVersion = "1.0-SNAPSHOT";
    packageName = "vehicles";
    packagePath = "vehicles";
}

task vehicles_pop by pop
{
    moduleName = "vehicles_pop";
    moduleVersion = "1.0";
}

task vehicles_cmlc_java by cmlc_java
{
    groupId = "vehicles";
    artifactId = "vehicles-cmlc";
    artifactVersion = "1.0-SNAPSHOT";
    packageName = "vehicles";
    packagePath = "vehicles";
}

task vehicles_cmlc_py by cmlc_py
{
    moduleName = "vehicles_cmlc";
    moduleVersion = "1.0";
}

