{
    groupId = "cml-inheritance-multiple";
    artifactId = "mcml-cmlc";
    artifactVersion = "1.0-SNAPSHOT";
    packageName = "mcml.cmlc";
    packagePath = "mcml/cmlc";
}

task mcml_py by cmlc_py
{
    moduleName = "cml_inheritance_multiple_mcml_cmlc";
    moduleVersion = "1.0";
